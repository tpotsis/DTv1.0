FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.000000000001;

boundaryField
{
    inlet
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    wallup
    {
        type            symmetry;
    }

    rc
    {
    type            calculated;
       value           $internalField; 
    }
  
    cylinder
    {
           type            calculated;
           value           $internalField;
    }
      
    walldown
     {
        type            symmetry;
     }
     lid
     {
       type             symmetry;
     }
}


// ************************************************************************* //
